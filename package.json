{
  "name": "8env-vscode-copy-utils",
  "displayName": "8env-vscode-copy-utils",
  "description": "copy file content",
  "version": "0.0.1",
  "publisher": "8env.kirilleremin",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ke-copy-all.copyFileContent"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ke-copy-all.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "ke-copy-all.copyFileContent",
        "title": "üìù Copy files content"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ke-copy-all.copyFileContent",
          "group": "2_workspace",
          "when": "resourceScheme == file"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.39.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
